<h1>Listing bill_friends</h1>

<table>
  <tr>
    <th>User</th>
    <th>Bill</th>
    <th>Contrinution</th>
  </tr>

<% @bill_friends.each do |bill_friend| %>
  <tr>
    <td><%= bill_friend.user_id %></td>
    <td><%= bill_friend.bill_id %></td>
    <td><%= bill_friend.contribution %></td>
  </tr>
<% end %>
</table>

